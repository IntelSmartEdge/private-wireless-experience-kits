---
image:
  repository: {{ openness_harbor_registry_address }}/{{ app_name }}
  tag: "{{ image_tag }}"                    # The tag identifying the upf docker image,
  pullPolicy: IfNotPresent                  # Pull policy for docker image

isPrivileged: false                         # Pod privilege level, possible values: true false
amf_smf_autostart: true
amf_smf_cera: true

#node information
node:
  path: {{ config_5gc_script_path }}         # Path on the node where the configuration
                                             # update script for AMF-SMF binary is stored

#update amf-smf configs
amf_smf_configs:
  pfd_config_file_path: {{ pfd_config_file_path }}
  sm_policy_config_file_path: {{ sm_policy_config_file_path }}
  local_dn_subnet: {{ local_dn_subnet }}
  routed_app_id: {{ routed_app_id }}
